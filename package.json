{
  "name": "ecom-api",
  "version": "1.0.0",
  "description": "E-commerce API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx --watch src/server.ts",
    "build": "tsup --config build.ts",
    "clean": "rm -r dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "author": "irfanfitriandi",
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "express": "^4.21.2",
    "got": "^14.4.5",
    "prisma": "^6.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/got": "^9.6.12",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  }
}
